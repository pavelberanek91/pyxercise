            <promenna>
                <znacka>t</znacka>
                <nazev></nazev>
                <jednotka>s</jednotka>
                <min>10</min>
                <max>30</max>
                <desetiny>0</desetiny>
            </promenna>
        </promenne>
        <vysledky>
            <vysledek>
                <nazev>s</nazev>
                <vzorec>0.5*{t}**2*({v}/({t}*3.6))</vzorec>
